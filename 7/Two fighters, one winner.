function declareWinner(fighter1, fighter2, firstAttacker) {
  let hp1= Math.ceil(fighter1.health / fighter2.damagePerAttack)
  let hp2= Math.ceil(fighter2.health / fighter1.damagePerAttack)
  console.log(hp1,fighter1.name,hp2,fighter2.name)
  return hp1>hp2? fighter1.name : hp2>hp1 ? fighter2.name : firstAttacker
}
