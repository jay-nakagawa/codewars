var humanYearsCatYearsDogYears = function(humanYears) {
 
  if (humanYears == 1) return [1,15,15]; //takes care of ages for year 1
  if (humanYears == 2) return [2,24,24] // takes care of ages for year 2
 return [humanYears, 24 + (humanYears - 2) * 4 , 24 + (humanYears - 2) * 5 ] //takes care of ages for years 3 or greater 

}
